{
  "name": "mediq",
  "version": "1.0.0",
  "private": true,
  "description": "This is a [Next.js](https://nextjs.org) + Express monorepo for MediQ",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "install:all": "cd backend && npm ci && cd ../frontend && npm ci",
    "lint": "npm --prefix frontend run lint && npm --prefix backend run lint",
    "test": "npm --prefix backend run test",
    "build": "npm --prefix frontend run build",
    "prepare": "husky install",
    "dev:backend": "npm --prefix backend run dev",
    "dev:frontend": "npm --prefix frontend run dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\""
  },
  "dependencies": {
    "@tabler/icons-react": "^3.34.0",
    "cliui": "^8.0.1",
    "color-convert": "^2.0.1",
    "color-name": "^1.1.4",
    "date-fns": "^2.30.0",
    "escalade": "^3.2.0",
    "get-caller-file": "^2.0.5",
    "has-flag": "^4.0.0",
    "leaflet-color-markers": "^0.1.0",
    "lodash": "^4.17.21",
    "node-geocoder": "^4.4.1",
    "p-throttle": "^7.0.0",
    "require-directory": "^2.1.1",
    "rxjs": "^7.8.2",
    "shell-quote": "^1.8.2",
    "spawn-command": "^0.0.2",
    "supports-color": "^8.1.1",
    "tree-kill": "^1.2.2",
    "tslib": "^2.8.1",
    "y18n": "^5.0.8",
    "yargs": "^17.7.2",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "husky": "^9.1.7"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
